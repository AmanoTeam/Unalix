{
  "providers": {
    "amp-project": {
      "urlPattern": "(.*google\\..*/amp/s/.*|/amp/|(\\?|&)amp\\b|\\.amp\\.htm)",
      "rules": [
        ".*google.*/amp/s/(.*) <-> \\1",
        "(/google)?/amp\\b <-> ",
        "\\.amp\\.htm <-> .htm"
      ]
    },
    "youtube": {
      "urlPattern": "https?://(www\\.|m\\.)?(youtube\\.com|youtu\\.be|youtube-nocookie\\.com).*",
      "rules": [
        "(www\\.|m\\.)?(youtube\\.com|youtu\\.be|youtube-nocookie\\.com) <-> invidio.us"
      ]
    },
    "twitter": {
      "urlPattern": "https?://(mobile\\.|www\\.)?twitter.com.*",
      "rules": [
        "(mobile\\.|www\\.)?twitter.com <-> nitter.net"
      ]
    },
    "instagram_user": {
      "urlPattern": "https?://(m\\.|www\\.)?instagram.com(?!/p/).*",
      "rules": [
        "(m\\.|www\\.)?instagram.com/ <-> bibliogram.pussthecat.org/u/"
      ]
    },
    "instagram_post": {
      "urlPattern": "https?://(m\\.|www\\.)?instagram.com/p/.*",
      "rules": [
        "(m\\.|www\\.)?instagram.com <-> bibliogram.pussthecat.org"
      ]
    },
    "all": {
      "urlPattern": ".*",
      "rules": [
        "(%26)+ <-> \\1",
        "(&)+ <-> \\1",
        "(%3f)+ <-> \\1",
        "(%3F)+ <-> \\1",
        "s/(\\?)+ <-> \\1",
        "(%3f|%3F|\\?|%23|&)+$ <-> ",
        "\\?& <-> ?",
        "%3f%26 <-> %3f",
        "%3F%26 <-> %3F"
      ]
    }
  }
}
